@article{adames_hpv_2023,
	title = {{HPV} {NA} {REGIÃO} {SUL} {DO} {BRASIL}: {COBERTURA} {VACINAL} {E} {ALTERAÇÕES} {NEOPLÁSICAS} {CERVICO}-{UTERINAS}},
	volume = {3},
	issn = {2447-0961, 2764-7757},
	shorttitle = {{HPV} {NA} {REGIÃO} {SUL} {DO} {BRASIL}},
	url = {https://ojs.revistacontemporanea.com/ojs/index.php/home/article/view/637},
	doi = {10.56083/RCV3N4-024},
	abstract = {Introdução: O HPV é um vírus que está associado a neoplasias malignas do trato genital, verrugas genitais e cutâneas. Considerando que o HPV é condição necessária para o câncer cervical, a vacinação para prevenção do HPV representa potencial para reduzir a carga de doença cervical e lesões precursoras. Objetivos: Avaliar os resultados da cobertura vacinal contra HPV, relacionar com lesões anatomopatológicas precursoras e outros indicadores do câncer de colo de útero. Materiais e Métodos: Estudo descritivo retrospectivo da região sul do Brasil, entre 2014-2019, com a população feminina alvo da companha de vacinação, excluindo idade de 9 anos. A faixa etária entre 10-19 anos e a população geral também foram avaliadas. Os dados foram coletados do Sistema de Informação em Saúde no site do DATASUS. Resultados: O ano com maior percentual de meninas vacinadas com primeira e segunda dose foi 2014, com 56,5\% e 35,8\% respectivamente. A partir de 2016, houve uma diminuição na vacinação, principalmente em relação à administração da primeira dose. Apesar dessa vacinação desde 2014, aumentou em 134\% os diagnósticos de Neoplasia Intraepitelial Cervical (NIC) na região, quando comparado o ano de início da campanha com 2019. A faixa etária de 14-19 anos obteve o maior número de casos, predominando NIC 1. O percentual de câncer de colo de útero aumentou, tanto de carcinoma (+63,6\%) quanto de adenocarcinoma no período. Discussão: A região sul brasileira não obteve taxa de vacinação próxima a meta de 80\% do Ministério da Saúde. Considerações Finais:  Faz-se necessário avaliar os programas de imunização atuais, buscando o controle das doenças precursoras pelo HPV evitáveis por imunização.},
	number = {4},
	urldate = {2024-12-04},
	journal = {Rev. Contemp.},
	author = {Adames, Mariela Goulart and Barbosa, Natália and Rojas, Roberta Ferreira and Rojas, Paulo Fernando Brum},
	month = apr,
	year = {2023},
	pages = {3149--3167},
	file = {Texto completo:/home/tdaitx/Zotero/storage/99SFJ2YJ/Adames et al. - 2023 - HPV NA REGIÃO SUL DO BRASIL COBERTURA VACINAL E ALTERAÇÕES NEOPLÁSICAS CERVICO-UTERINAS.pdf:application/pdf},
}

@incollection{melo_avancos_2021,
	edition = {1},
	title = {{AVANÇOS} {E} {DESAFIOS} {NA} {COBERTURA} {VACINAL} {BRASILEIRA} {NOS} Ú{LTIMOS} {VINTE} {ANOS}: {UM} {ESTUDO} {EPIDEMIOLÓGICO}},
	isbn = {9786599557217},
	shorttitle = {{AVANÇOS} {E} {DESAFIOS} {NA} {COBERTURA} {VACINAL} {BRASILEIRA} {NOS} Ú{LTIMOS} {VINTE} {ANOS}},
	url = {https://literaciacientificaeditora.com.br/catalogos/cenarios-da-saude-coletiva-no-brasil/},
	abstract = {OBJETIVO: Analisar os dados epidemiológicos da vacinação brasileira do ano 2000 a 2020 e refletir sobre as variações na cobertura vacinal no Brasil e possíveis eventos interferentes no PNI. MATERIAIS E MÉTODOS: Estudo descritivo e transversal a partir da análise dos dados do DATASUS sobre a cobertura vacinal brasileira nos últimos 20 anos, coletados em maio de 2021, com o auxílio do Microsoft Office Word 2016 e Microsoft Office Excel 2016 foram comparados quanto aos percentuais alcançados, variação anual e distribuição regional. RESULTADOS E DISCUSSÃO: Observou-se um impacto positivo da vacinação a partir do Programa Nacional de Imunização (PNI), propiciando a erradicação de algumas doenças antes consideradas problemas de saúde pública. No entanto, apesar dos níveis crescentes de cobertura vacinal até 2015, quando atingiu o percentual máximo, houve decréscimo nos anos subsequentes, podendo estar relacionado à difusão das fake news e aos movimentos antivacina. CONCLUSÃO: Constatou-se que apesar dos avanços obtidos a partir da adesão social ao PNI, o advento das fake news e as campanhas antivacina têm se mostrado verdadeiros desafios à saúde pública, inclusive durante a pandemia da Covid-19, e podem ter repercussões para além dos índices de cobertura vacinal, visto que possibilitam a perpetuação de doenças que poderiam ser erradicadas pela vacinação.},
	urldate = {2024-12-04},
	booktitle = {{CENÁRIOS} {DA} {SAÚDE} {COLETIVA} {NO} {BRASIL}},
	publisher = {Literacia Cientifica Editora \& Cursos},
	author = {Morais, Vanessa Maria Oliveira and Santos, Lucas Siqueira Dos and Silva, Francielly Azevedo Da and Melo, Luís Ricardo Santos De and Santos, Vitória Dias Dos and Tavares, Anna Beatriz Lopes and Alves, Rebeca Feitosa Dória and Souza, Júllia Beatriz Araujo and Santos, Nayara De Oliveira Santiago and Campos, André Luiz Baião},
	collaborator = {Melo, Mayara Macêdo},
	year = {2021},
	doi = {10.53524/lit.edt.978-65-995572-1-7/04},
	pages = {37--47},
	file = {Texto completo:/home/tdaitx/Zotero/storage/E4ZSYAFM/Morais et al. - 2021 - AVANÇOS E DESAFIOS NA COBERTURA VACINAL BRASILEIRA NOS ÚLTIMOS VINTE ANOS UM ESTUDO EPIDEMIOLÓGICO.pdf:application/pdf},
}

